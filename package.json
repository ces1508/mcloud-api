{
  "name": "mepscloud-api",
  "version": "1.0.0",
  "description": "mepscloud rest api",
  "main": "index.js",
  "scripts": {
    "lint": "standart",
    "test": "npm run lint && ava --require babel-register test/**-test.js",
    "users": "micro -p5000 users.js",
    "auth": "micro -p6000 auth.js",
    "databases": "micro -p5005  databases.js",
    "contacts": "micro -p5001 contacts.js",
    "templateEmails": "micro -p5100 templateEmails.js",
    "campaign-emails": "micro -p4200 campaingEmails.js",
    "upload-images": "micro -p5003 images.js",
    "hook": "micro -p5000 hook.js ",
    "send-Emails": "micro -p4100 sendEmails.js",
    "campaign-sms": "micro -p5002 campaignSms.js",
    "send-sms": "micro -p4000 sendSms.js",
    "reports": "micro -p5004 reports.js",
    "submit": "npm pack && cp mepscloud-api-1.0.0.tgz ../../Ansible_MepsCloud/roles/mepscloud_api/files && tar xvfz mepscloud-api-1.0.0.tgz && mv package mepscloud-api"
  },
  "standart": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "author": "christian eduardo segura <ces1508@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^7.0.0",
    "babel-register": "^6.16.3",
    "standart": "^6.0.0",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "each-async": "^1.1.1",
    "http-hash": "^2.0.0",
    "jsonwebtoken": "^7.1.9",
    "micro": "^4.1.1",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "rethinkdb": "^2.3.3",
    "sendgrid": "^4.5.0",
    "token-extractor": "^0.1.6",
    "uuid-base62": "^0.1.0"
  }
}
